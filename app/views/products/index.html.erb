<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Pages#index</h1>
      <ul>
        <% Product::CATEGORY.each do |category| %>
          <li>
            <%= link_to category, products_path(category: category) %>
          </li>
        <% end %>
<body>
  <div class="container" style="background-color: #DAD7CD">
    <div class="row">
      <div class="col-xs-12 col-sm-12 col-sm-offset-1">
        <h1>Pages#index</h1>
        <ul class="list-group mb-3">
              <% @products.each do |product| %>
                <li class="list-group-item">
                <div class="card">
                  <%= link_to product.name, product_path(product) %>
                  <%= cl_image_tag product.photos.first.key, alt: product.name %>
                  <p class="price"><%= number_with_precision(product.price, precision: 2) %></p>
                </div>
                </li>
          <% end  %>
        </ul>
      </div>
    </div>
  </div>
</body>
